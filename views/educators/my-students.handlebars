<!-- educators\my-students.handlebars -->
{{> nav/loggedineducator}}

<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3>{{data.project.name}}</h3>
    </div>
    <div class="panel-body">
      <table class="table table-striped table-hover ">
        <thead>
          <tr>
            <th>Username</th>
            <th>Link to Student Data</th>
            <th>Email</th>
          </tr>
        </thead>
		    <tbody>
		    	{{#each data.students}}
			      <tr>
			        <td><a href="/educator/student-data/{{id}}">{{username}}</a></td>
			        <td><a href="/educator/student-data/{{id}}""><span class="glyphicon glyphicon-file"></span></a></td>
			        <td>{{email}}</td>
			      </tr>
		      {{/each}}
		    </tbody>
		  </table> 
		</div>
	</div>
</div>


