<div class="container">

  </div>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3>My Data</h3>
      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover ">
          <thead>
            <tr>
              <th>Project</th>
              <th>Link</th>
              <th>Title</th>
              <th>Description</th>
              <th>Date</th>
            </tr>
          </thead>
          <tbody>
          {{#each observations}}
            <tr>
              <td>{{Project.name}}</td>
              <td><a href="{{doclink}}"><span class="glyphicon glyphicon-file"></span></a></td>
              <td>{{title}}</td>
              <td>{{description}}</td>
              <td>{{notedate}}</td>
            </tr>
		  {{/each}}
          </tbody>
        </table> 

      </div>
    </div>

</div>






